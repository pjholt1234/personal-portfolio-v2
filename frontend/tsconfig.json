{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@helpers": ["src/helpers.ts"],
      "@types": ["src/types.d.ts"],
      "@utils": ["src/utils/index.ts"],
      "@hooks": ["src/hooks/*"],
      "@enums": ["src/enums/index.ts"],
      "@api": ["src/api.ts"],
      "@global": ["src/components/Global/index.ts"],
      "@shared-ui": ["src/components/shared-ui/index.ts"],
      "@projects": ["src/components/Projects/index.ts"],
      "@events": ["src/components/Events/index.ts"],
      "@home": ["src/components/Home/index.ts"],
      "@project": ["src/components/Project/index.ts"],
      "@event": ["src/components/Event/index.ts"]
    }
  },
  "include": ["src"],
  "references": []
}
